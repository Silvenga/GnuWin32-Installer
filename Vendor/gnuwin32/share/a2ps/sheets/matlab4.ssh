optional operators are
   ~  \not,
   |  \vee,
   &  \wedge,
   ~= \neq,
   <= \leq,
   >= \geq,
   == \equiv,
   xor \oplus
end operators

sequences are
   "% " Comment,
   "disp" Keyword Plain "')",
   "error" Keyword Plain "')",
   "fprintf" Keyword Plain "',"
end sequences

end style
