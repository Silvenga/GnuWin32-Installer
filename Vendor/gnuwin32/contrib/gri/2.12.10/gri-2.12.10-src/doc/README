To update the SourceForge site:
 make gri.pdf
 make html-tar 
 scp gri.pdf gridoc.tar.gz  dankelley@gri.sourceforge.net:/home/groups/g/gr/gri/htdocs
 ssh dankelley@gri.sourceforge.net
	cd /home/groups/g/gr/gri/htdocs
	\rm gridoc
	tar zxvf gridoc.tar.gz  # may have timestamp problems
	rm gridoc.tar.gz
	exit
 cd sourceforge
	sh ./INSTALL

